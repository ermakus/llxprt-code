{
  "name": "yandex",
  "baseProvider": "openai",
  "baseUrl": "https://llm.api.cloud.yandex.net/v1",
  "defaultModel": "gpt://${FOLDER_ID}/yandexgpt/rc",
  "description": "Yandex Cloud YandexGPT. Set YANDEX_API_KEY and YANDEX_FOLDER env vars.",
  "apiKeyEnv": "YANDEX_API_KEY",
  "envVars": {
    "FOLDER_ID": "YANDEX_FOLDER"
  },
  "providerConfig": {
    "customHeaders": {
      "x-folder-id": "${FOLDER_ID}"
    },
    "parallel_tool_calls": false
  }
}
